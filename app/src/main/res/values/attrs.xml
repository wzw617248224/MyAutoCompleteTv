<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DrawableCheckBox">
        <attr name="drawable_src" format="reference" />
        <attr name="drawable_height" format="dimension" />
        <attr name="drawable_width" format="dimension" />
        <attr name="drawable_location">
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CirCleRefreshLayout">
        <attr name="AniBackColor" format="color" />
        <attr name="AniForeColor" format="color" />
        <attr name="CircleSmaller" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CustomRadiusLl">
        <attr name="leftTopRadius" format="dimension"/>
        <attr name="rightTopRadius" format="dimension"/>
        <attr name="leftBottomRadius" format="dimension"/>
        <attr name="rightBottomRadius" format="dimension"/>
        
        <attr name="leftTopInside" format="boolean"/>
        <attr name="rightTopInside" format="boolean"/>
        <attr name="leftBottomInside" format="boolean"/>
        <attr name="rightBottomInside" format="boolean"/>

        <attr name="divideLineDirection">
            <enum name="RIGHT" value="0"/>
            <enum name="LEFT" value="1"/>
        </attr>
        <attr name="bgDrawable" format="reference"/>
    </declare-styleable>
</resources>